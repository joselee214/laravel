{
    "name": "joselee214/laravel",
    "description": "Ypc ... Components for Laravel Framework code generation. focker from reliese/laravel @ Cristian Llanos .",
    "keywords": ["Joselee214", "laravel"],
    "support": {
        "source": "https://github.com/joselee214/laravel"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jose",
            "email": "jose@7yes.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": ">=2.5",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Joselee214\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joselee214\\Ypc\\Joselee214ServiceProvider"
            ]
        }
    }
}
